
<%
    contacts = Contact.all   
%>


<h1> Contacts
</h1>

<table>
<tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Company</th>
</tr>
<% for contact in contacts %>
    <% company = Company.find_by({ "id" => contact ["company_id"]})%>
<tr>
    <td><%= contact["first_name"]%></td>
    <td><%= contact["last_name"]%></td>
    <td><%= contact["email"]%></td>
    <td><%= company["name"]%></td>

</tr>
<%end%>

<p>
</p>